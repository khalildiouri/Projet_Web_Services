<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/HibernateMapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    <hibernate-mapping>
    	<class name="com.upem.selling.beans.UserWS" table="user">
    		<id name="id_user" column="id_user">
    		</id>
    		<property name="firstname" column="firstname" ></property>
    		<property name="name" column="name"></property>
    		<property name="username" column="username"></property>
    		<property name="password" column="password"></property>
    		<property name="email" column="email"></property>
    		<property name="role" column="role"></property>
    		<property name="adresse" column="adresse"></property>
    		<property name="brithDate" column="brithDate"></property>
    	
    	</class>
    </hibernate-mapping>